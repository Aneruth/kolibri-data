{
        "WORD" : "(?P<WORD>[^.'\\s,#:!;/\\({\\[\\]\\)}?-]+|(?:'s|'t))",
        "NUM" : "(?P<NUM>\\d+)",
        "CODE" : "(?P<NUMCODE>\\b[A-Z0-9]*(?=[A-Z0-9]{4,10})(?=[A-Z0-9]*\\d)(?=[A-Z0-9]*)[A-Z0-9-]*\\b\"",
